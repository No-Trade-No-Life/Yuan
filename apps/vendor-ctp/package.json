{
  "name": "@yuants/vendor-ctp",
  "version": "0.4.14",
  "main": "lib/index.js",
  "files": [
    "dist",
    "lib",
    "scripts",
    "ctp"
  ],
  "scripts": {
    "postinstall": "node ./scripts/post-install.js",
    "build": "heft test --clean && node ./lib/scripts/build-ctp-types.js && node ./lib/scripts/build-ctp-bridge.js && docker buildx build --target build_env -t build-env -f ./build/Dockerfile . 2>/dev/null && yuan-toolkit post-build && docker run --rm -v ./ctp:/app -w /app --user $(id -u):$(id -g) build-env /bin/bash -c 'rm -rf build && mkdir -p build && cd build && cmake .. && make && find . -mindepth 1 -name 'main_linux' -prune -o -exec rm -rf {} +'"
  },
  "dependencies": {
    "@yuants/data-account": "workspace:*",
    "@yuants/data-order": "workspace:*",
    "@yuants/protocol": "workspace:*",
    "@yuants/utils": "workspace:*",
    "@yuants/sql": "workspace:*",
    "@yuants/data-product": "workspace:*",
    "binary-parser-encoder": "~1.5.3",
    "date-fns": "~2.29.3",
    "rxjs": "~7.5.6",
    "zeromq": "6.4.2"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "~7.30.0",
    "@rushstack/heft": "~0.47.5",
    "@rushstack/heft-jest-plugin": "~0.16.8",
    "@rushstack/heft-node-rig": "~1.10.7",
    "@types/heft-jest": "1.0.3",
    "@types/json-schema": "~7.0.11",
    "@types/node": "22",
    "@yuants/extension": "workspace:*",
    "@yuants/tool-kit": "workspace:*",
    "typescript": "~4.7.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "io_ntnl": {
    "deploy_files": [
      "dist",
      "lib",
      "temp",
      "build",
      "ctp"
    ]
  }
}
